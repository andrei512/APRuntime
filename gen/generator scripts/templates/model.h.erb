<%
=begin

Model Interface Template

name - model name
properties - array containing hashes that describe the properties
	:name - property name
	:json_name - the key from the JSON returned by a WS that uses the model
	:default - the default value
	:type - the type for the property (ex. NSString, NSArray)
	:documentation 
	:of - for Arrays the type of the contained object can be given
		:of - the class
		:

namespace_name - 
classes_to_import - 
=end
%>
<%= header_for_file "#{name}.h" %>

#import "Model.h"

<% classes_to_import.each do |to_import| %>#import "<%= to_import %>.h"
<% end %>

@interface <%= name %> : Model

<% properties.each do |property| %><% if property["documentation"] %><%= "\n" + pretty_comment(property["documentation"]) %><% end %>
@property (nonatomic, strong) <%= "#{property["type"]} *#{property["name"]}" %>;
<% end %>


@end

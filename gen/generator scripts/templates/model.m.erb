<%
=begin

Model Implementation Template

name - model name
properties - array containing hashes that describe the properties
	:name - property name
	:json_name - the key from the JSON returned by a WS that uses the model
	:default - the default value
	:type - the type for the property (ex. NSString, NSArray)
	:documentation 
	:of - for Arrays the type of the contained object can be given
		:of - the class
		:
namespace_name - 
classes_to_import - 
=end

%>
<%= header_for_file "#{name}.m" %>

#import "<%= name %>.h"
#import "NSArray+Utils.h"

@implementation <%= name %> 

- (NSArray *)propertyMetadata {
	return <%= to_objc(properties.map { |p| h = p.clone; h.delete("documentation"); h}, 
					   1, 
					   true) %>;
}


@end
